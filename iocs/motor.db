  field(INPB,"5")
  field(INPC,"XF:31IDA-OP{Tbl-Ax:X1}Mtr:RBV CP")
  field(INPD,"2.5")
  field(INPE,"0.3")
  field(CALC,"C<A||C>B?RNDM*E:EXP(-1.0*((C-D)**2))")
