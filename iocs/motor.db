record(ao, "XF:31IDA-OP{Tbl-Ax:X1}Mtr:SP") {
    field(SCAN, "1 second")
}

record(calc, "XF:31IDA-OP{Tbl-Ax:X1}Mtr:RBV") {
    field(VAL, "0")
    field(SCAN, ".1 second")
    field(INPA, "XF:31IDA-OP{Tbl-Ax:X1}Mtr:SP")
    field(CALC, "VAL<A?VAL+1:VAL>A?VAL-1:VAL")
    field(HIHI, "10")
    field(HHSV, "MAJOR")
    field(HIGH, "8")
    field(HSV, "MINOR")
    field(LOW, "-8")
    field(LSV, "MINOR")
    field(LOLO, "-10")
    field(LLSV, "MAJOR")
}

